{
  "name": "lisk-paper-wallet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.1.1",
    "react-detect-offline": "^1.0.6",
    "react-dom": "^16.1.1",
    "react-easy-transition": "^1.2.6",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js --windows",
    "ypreelectron-pack": "yarn build"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "electron": "^1.7.9",
    "electron-builder": "^19.47.1",
    "wait-on": "^2.0.2"
  },
  "build": {
    "appId": "com.liskpaper",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
