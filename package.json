{
  "name": "liskpaper",
  "version": "1.0.0",
  "private": true,
  "main": "public/electron.js",
  "author": "Carbonara & Lisk Elite",
  "description": "Do you want to store your Lisk safely? Do you want to gift Lisk to somebody? Or maybe you just want to generate an offline wallet for Lisk? liskpaper.com will do it for you! built by Carbonara & Lisk Elite",
  "homepage": "./",
  "dependencies": {
    "bip39": "^2.4.0",
    "browserify-bignum": "1.3.0-2",
    "crypto": "^1.0.1",
    "electron-is-dev": "^0.3.0",
    "font-awesome": "^4.7.0",
    "js-nacl": "^1.2.2",
    "prop-types": "^15.6.0",
    "qrcode.react": "^0.7.2",
    "qs": "^6.5.1",
    "react": "^16.1.1",
    "react-detect-offline": "^1.0.6",
    "react-dom": "^16.1.1",
    "react-easy-transition": "^1.2.6",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js --mac --windows --linux",
    "preelectron-pack": "yarn build"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "electron": "^1.7.9",
    "electron-builder": "^19.47.1",
    "wait-on": "^2.0.2"
  },
  "build": {
    "appId": "com.liskpaper",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
